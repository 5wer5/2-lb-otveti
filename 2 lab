// Example program
#include <iostream>
#include <string>
#include <cmath>

using namespace std;
int main()
{
    int A ;
    cin >> A ;
    
    if( A%2 == 0   )
    {cout << "Even";
    }
    else{ cout << "Odd";
    }
    
    
    
}
2) // Example program
#include <iostream>
#include <string>
#include <cmath>

using namespace std;
int main()
{
    int A, B;
    cin >> A >> B ;
    
    
    
    if( A%2 == 0 && B%2 ==0   )
    {cout << "odinakovoe";
    }
    
    else{ cout << "raznoe";
    }
    
    
    
}
3) // Example program
#include <iostream>
#include <string>
#include <cmath>

using namespace std;
int main()
{
    int A, B ,C;
    cin >> A >> B >> C ;
    
    
    
    if( A > 0 && B > 0    )
    {cout << "+";
    }
    else if ( A > 0 && C > 0)
    {cout << "+";
    }
    else if (B> 0 && C> 0)
    {cout << "+";
    }
    else{ cout << "-";
    }
    
    
    
}
