16) // Example program
#include <iostream>
#include <string>
#include <cmath>

using namespace std;
int main()
{
    int a , b  ;
    cin >> a   ;
    
    
    if (a > 0 ) {
    cout << "a +";
    
    }
    else if ( a == 0) {
        cout << " a = 0";
    }
    else {
        cout << "a -"; 
    }
    
    
    
    
    
}
    
