10) // Example program
#include <iostream>
#include <string>
#include <cmath>

using namespace std;
int main()
{
    int a , b , c, d= 0 ,k = 0;
    cin >> a >> b >> c ;
    
    if (a > 0) 
    d = d+1;
    else 
    k = k+1;
    if (b > 0)
    d = d+1;
    else
    k = k+1;
    if (c > 0)
    d = d+1;
    else
    k = k++;
    cout << d << k;
    }
    11) // Example program
#include <iostream>
#include <string>
#include <cmath>

using namespace std;
int main()
{
    int a , b  ;
    cin >> a >> b  ;
    
    if (a > b ) {
    a= a +b ;
    b= a -b ;
    a = a -b;
    cout << a << b;
    }

    else{
    
    cout << a << b;
    }
    
    
    
}
12) // Example program
#include <iostream>
#include <string>
#include <cmath>

using namespace std;
int main()
{
    int a , b , c , d ;
    cin >> a >> b >> c  ;
    
    if (a > c && b >c ) {
    d = a +b;
    
    }
    else if (a > b &&  c > b ) { 
        d = c +a;
    }
     
    else{
    d = b +c; 
    }
    cout << d ;
    
    
    
    
}
13) // Example program
#include <iostream>
#include <string>
#include <cmath>

using namespace std;
int main()
{
    int a , b , c , d, e ;
    cin >> a >> b >> c >> d  ;
    
    if (a == c && c ==b ) {
    e = 4;
    
    }
    else if (a == c &&  c ==d ) { 
        e = 2;
    }
    else if (a == b &&  b==d ) { 
        e = 3;
    }
    else {
        e = 1; 
    }
    cout << e;
    
    
    
    
}
14) // Example program
#include <iostream>
#include <string>
#include <cmath>

using namespace std;
int main()
{
    int a , b , c , d, e ;
    cin >> a >> b >> c   ;
    d = a-b;
    e= a - c;
    if (d > e ) {
    cout << "a close b";
    
    }

    else {
        cout << "a close c"; 
    }
    
    
    
    
    
}
